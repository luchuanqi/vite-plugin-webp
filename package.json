{
  "name": "vite-plugin-webp",
  "version": "1.1.3",
  "description": "a plugin of vite for transform webp",
  "main": "lib/vite-plugin-webp.js",
  "module": "lib/vite-plugin-webp.esm.js",
  "exports": {
    "import": "./lib/vite-plugin-webp.esm.js",
    "require": "./lib/vite-plugin-webp.js"
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w",
    "build": "rollup -c",
    "publish": "npm run build && npm publish",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vite-plugin-webp",
    "webp",
    "clip",
    "vite",
    "sharp"
  ],
  "author": "luchuanqi",
  "license": "MIT",
  "dependencies": {
    "cssjson": "^2.1.3",
    "sharp": "^0.30.7"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/node": "^18.6.5",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "rollup": "^2.77.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luchuanqi/vite-plugin-webp.git"
  },
  "homepage": "https://github.com/luchuanqi/vite-plugin-webp/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/luchuanqi/vite-plugin-webp/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
